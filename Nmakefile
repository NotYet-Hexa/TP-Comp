
SRCPP = Automate.cpp Etat.cpp Expression.cpp Lexer.cpp main.cpp Symbole.cpp
SRCPP_ETAT = Etats/$*.cpp
SRCH = Automate.h Etat.h Expression.h Lexer.h Constante.h Symbole.h
SRCH_ETAT = Etats/*.h

SRC = $(SRCH) $(SRCH_ETAT) $(SRCCPP) $(SRCCPP_ETAT)
OBJ = $(SRCCPP:.cpp=.o)


all: exe.exe
	echo $(OBJ)

exe.exe: $(OBJ) $(SRC)
	echo $(OBJ)
	$(CPP) $(CPPFLAGS) 

.cpp.obj:
	$(CPP) $(CPPFLAGS) 

exe.exe: *.cpp




